// Mod support for Tabula Rasa custom filters
// Adds two new filters: "My Enternia" and "Alta"
// Each filter is a button that applies an according group as a recipe filter in the Tabula Rasa crafting table interface.
[
    { "op":"add", "path":"/filterButtons/-", "value": {
        "baseImage" : "/objects/wired/tabularasa/my_enternia.png",
        "baseImageChecked" : "/objects/wired/tabularasa/my_enternia_selected.png",
        "filter" : "my_enternia"
    } },
    { "op":"add", "path":"/filterButtons/-", "value": {
        "baseImage" : "/objects/wired/tabularasa/alta.png",
        "baseImageChecked" : "/objects/wired/tabularasa/alta_selected.png",
        "filter" : "alta"
    } }
]
